{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/programm",
      "destination": "/programm.html"
    },
    {
      "source": "/weitere-informationen",
      "destination": "/weitere-informationen.html"
    },
    {
      "source": "/support",
      "destination": "/support.html"
    }
  ]
}
