{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/info",
      "destination": "/info.html"
    },
    {
      "source": "/impressum",
      "destination": "/impressum.html"
    },
    {
      "source": "/datenschutz",
      "destination": "/datenschutz.html"
    },
    {
      "source": "/passwortverlust",
      "destination": "/passwortverlust.html"
    },
    {
      "source": "/account",
      "destination": "/account.html"
    }
  ]
}